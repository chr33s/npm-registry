{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/**",
        "function": "npm"
      }
    ]
  },
  "functions": {
    "ignore": [
      "node_modules",
      "public",
      "*/.*",
      ".firebase",
      "app.yaml",
      "Dockerfile",
      "LICENSE",
      "README.md",
      "server.js",
      "test.js"
    ],
    "source": "."
  }
}
